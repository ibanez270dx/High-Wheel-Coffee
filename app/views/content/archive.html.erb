<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Growing Region</th>
      <th>Cultivar</th>
    </tr>
  </thead>
  <tbody>
    <% @coffees.each do |coffee| %>
      <tr data-url="<%= coffee_path(coffee.url_name) %>" class="clickable">
        <td><%= coffee.name %></td>
        <td><%= coffee.growing_region %></td>
        <td><%= coffee.cultivar %></td>
      </tr>
    <% end %>
  </tbody>
</table>