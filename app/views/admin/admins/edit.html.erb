<h2>Editing <%= @admin.name %></h2>

<%= form_for [:admin, @admin], html: { class: 'form-horizontal' } do |f| %>
  <%= render partial: "shared/error-messages", locals: { object: @admin } %>
	<%= render partial: "form", locals: { f: f } %>
	
	<div class="form-actions well">
    <%= f.submit 'Update Admin', class: 'btn btn-primary' %>
    <%= link_to 'Cancel', admin_admins_path, class: 'btn' %>
  </div>
<% end %>